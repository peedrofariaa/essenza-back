generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Usuario {
  id           Int      @id @default(autoincrement())
  name         String
  email        String   @unique
  cpf          String   @unique
  birth        DateTime
  password     String
  criadoEm     DateTime @default(now())
  resetToken   String?
  resetExpires DateTime?
}